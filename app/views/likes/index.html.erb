<h1> Likes </h1>
<div class = "row">
    <div class = "col-md-8">
        <% @likes.each do |like| %>
            <div class="media">
                <div class="media-body">
                    <h4 class="media-heading">
                        <%=  like.bookmark.url %>
                    </h4>
                </div>
            </div>
        <% end %>
    </div>
</div>
